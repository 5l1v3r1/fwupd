From d1efa0e2b5c45f552858d719cd53e56bfa4c6dbb Mon Sep 17 00:00:00 2001
From: Mario Limonciello <mario.limonciello@dell.com>
Date: Thu, 9 Nov 2017 18:29:59 -0600
Subject: [PATCH] debian: only do libsmbios on supported architectures

---
 contrib/ci/dependencies.xml | 5 ++++-
 1 file changed, 4 insertions(+), 1 deletion(-)

diff --git a/contrib/ci/dependencies.xml b/contrib/ci/dependencies.xml
index bd4962b..022c17b 100644
--- a/contrib/ci/dependencies.xml
+++ b/contrib/ci/dependencies.xml
@@ -607,7 +607,10 @@
       <package>libsmbios-devel</package>
     </distro>
     <distro id="debian">
-      <control />
+      <control>
+        <inclusive>i386</inclusive>
+        <inclusive>amd64</inclusive>
+      </control>
       <package variant="x86_64" />
       <package variant="i386" />
     </distro>
-- 
2.7.4

